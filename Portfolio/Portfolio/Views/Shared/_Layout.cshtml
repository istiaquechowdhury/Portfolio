<body>
    <!-- ======= Mobile nav toggle button ======= -->
    <i class="bi bi-list mobile-nav-toggle d-xl-none"></i>

    <!-- ======= Header ======= -->
    <header id="header">
        <div class="d-flex flex-column">

            <div class="profile">
                <img src="assets/img/Istiaque.jpg" alt="" class="img-fluid rounded-circle">
                <h1 class="text-light"><a href="index.html">Istiaque Chowdhury</a></h1>
                <div class="social-links mt-3 text-center">
                    <a href="https://github.com/istiaquechowdhury" class="twitter"><i class="bi bi-github"></i></a>
                    <a href="https://www.facebook.com/istiaque.chowdhury.37/" class="facebook"><i class="bx bxl-facebook"></i></a>
                   
                    <a href="https://www.linkedin.com/in/istiaquechowdhury430/" class="linkedin"><i class="bx bxl-linkedin"></i></a>
                </div>
            </div>

            <nav id="navbar" class="nav-menu navbar">
                <ul>Is
                    <li><a asp-controller="Home" asp-action="Index" class="nav-link scrollto active"><i class="bx bx-home"></i> <span>Home</span></a></li>
                    <li><a asp-controller="About" asp-action="Index" class="nav-link scrollto"><i class="bx bx-user"></i> <span>About</span></a></li>
                    <li><a asp-controller="Skills" asp-action="Index" class="nav-link scrollto"><i class="bx bx-file-blank"></i> <span>Skills</span></a></li>
                    <li><a asp-controller="Portfolio" asp-action="Index" class="nav-link scrollto"><i class="bx bx-book-content"></i> <span>Portfolio</span></a></li>
                    <li><a asp-controller="Contact" asp-action="Index" class="nav-link scrollto"><i class="bx bx-server"></i> <span>Contact</span></a></li>
                   
                </ul>
            </nav><!-- .nav-menu -->
        </div>
    </header><!-- End Header -->
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer id="footer"style="margin-top:300px">
        <div class="container">
            <div class="copyright">
                &copy; <strong>Istiaque 2024<span></span></strong>
            </div>
         
        </div>
    </footer>


    
    

    <!--templade code starts from here-->
